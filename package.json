    "prettier-plugin-tailwindcss": "^0.4.0",
    "rimraf": "3.0.2",
    "simple-git-hooks": "2.8.0",
    "stylelint": "14.10.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "26.0.0",
